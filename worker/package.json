{
	"name": "@simon/worker",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"prebuild": "rm -rf dist",
		"build": "pnpm run prebuild && vite build",
		"deploy": "pnpm run build && pnpm exec wrangler deploy",
		"dev": "pnpm run build && wrangler dev",
		"lint": "eslint .",
		"start": "wrangler dev",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@simon/shared": "workspace:*"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240620.0",
		"wrangler": "^3.60.3"
	}
}
